<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="a.b">
    <!--    用反射调用方法
    id: 方法名字
    resultType：返回值类型
        返回值是集合： 集合的泛型
        返回值是对象： 对象的全路径
    -->
    <select id="selectAll" resultType="com.ylb.entity.Flower">
        SELECT * from flower
    </select>
</mapper>